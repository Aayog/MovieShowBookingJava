<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="com.myshowbooking.main.movie.model">
	<class name="UpComingCrudMovies" table="up_coming_movie_table">
		<id name="id" column="ID">
			<generator class="uuid2" />

		</id>
		<property name="moviename" column="MOVIE_NAME" />
		<property name="releasedate" column="RELEASE_DATE" />
		<property name="movieimageurl" column="MOVIE_IMAGE_URL" />
		<property name="bannerimageurl" column="BANNER_IMAGE_URL" />
		<property name="youtubelink" column="YOUTUBE_LINK" />
		<property name="facebooklink" column="FACEBOOK_LINK" />
		<property name="bookingenabled" column="BOOKING_ENABLED" />
		<property name="bookingavailabledate" column="BOOKING_AVAILABLE_DATE" />
		<property name="star" column="STAR" />
		<property name="comments" column="COMMENTS" />
		<property name="description" column="DESCRIPTION" />
		<property name="message" column="MESSAGE" />
		<property name="createddate" column="CREATED_DATE" />
		<property name="createdby" column="CREATED_BY" />
		<property name="deleteflag" column="DELETE_FLAG" />
		<property name="updatedate" column="UPDATE_DATE" />
		<property name="updateby" column="UPDATE_BY" />
		<property name="comment" column="COMMENT" />
		
    	</class>
    	</hibernate-mapping>
    	